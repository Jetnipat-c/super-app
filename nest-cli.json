{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "monorepo": true,
  "root": "apps/gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "account": {
      "type": "application",
      "root": "apps/account",
      "entryFile": "main",
      "sourceRoot": "apps/account/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account/tsconfig.app.json"
      }
    },
    "deposit": {
      "type": "application",
      "root": "apps/deposit",
      "entryFile": "main",
      "sourceRoot": "apps/deposit/src",
      "compilerOptions": {
        "tsConfigPath": "apps/deposit/tsconfig.app.json"
      }
    },
    "withdraw": {
      "type": "application",
      "root": "apps/withdraw",
      "entryFile": "main",
      "sourceRoot": "apps/withdraw/src",
      "compilerOptions": {
        "tsConfigPath": "apps/withdraw/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}
